    "deploy:prod:sorry": "node ./scripts/deploy-prod-sorry.js",
    "deploy:prod:backend": "node ./scripts/deploy-prod-backend.js",
    "deploy:prod:backend:onlyapi": "node ./scripts/deploy-prod-backend.js onlyapi",
    "deploy:uat:backend": "node ./scripts/deploy-uat-backend.js",
    "deploy:uat:backend:onlyapi": "node ./scripts/deploy-uat-backend.js onlyapi",
    "cms:pull": "node ./scripts/cms-pull.js",
    "format": "prettier --write \"**/*.{js,tsx,jsx,json,md}\"",
    "serve:uat": "npm run _deploy:uat:build && gatsby serve --prefix-paths",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "ngrok": "C:/utils/ngrok/ngrok.exe http 8000",
